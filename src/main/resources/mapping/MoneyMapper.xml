<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bwsk.mapper.MoneyMapper">
 
  	<!-- 删除所有 -->
 	<delete id="deleteCollectionMoney" parameterType="com.bwsk.entity.CollectionMoney">
 		delete from collectionmoney
 	</delete>
 
	<!-- 批量插入 -->
 	<insert id="insertCollectionMoney" parameterType="java.util.List">
 		insert into collectionmoney (money,mtime,mcomment,mstatus,pid,mtimeChinese)
 		  values 
	    <foreach collection="list" item="item" index="index" separator=",">
	        (#{item.money},#{item.mtime},#{item.mcomment},#{item.mstatus},#{item.pid},#{mtimeChinese})
	    </foreach>

 	</insert>
</mapper>